<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jun.financialrecords" >

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS"/>
    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".RecordsListActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AddRecordsActivity"
            android:label="@string/title_activity_add_records" >
        </activity>
        <activity
            android:name=".SettingActivity"
            android:label="@string/title_activity_setting" >
        </activity>
        <activity
            android:name=".InvestTypeManagerActivity"
            android:label="@string/title_activity_invest_type_manager" >
        </activity>
        <activity
            android:name=".ProjectDetailListActivity"
            android:label="@string/invest_detail" >
        </activity>
        <activity
            android:name=".ProjectDetailAddActivity"
            android:label="@string/title_activity_project_detail_add" >
        </activity>
    </application>

</manifest>
